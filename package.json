    "@actions/github": "^4.0.0",
    "@octokit/core": "^3.2.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.24.0",
    "eslint": "^7.4.0",
    "jest": "^26.1.0"
  }
